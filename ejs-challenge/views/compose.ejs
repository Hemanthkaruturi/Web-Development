<%- include('partials/header') -%>

<h1>Compose</h1>

<form class="" action="response" method="post">
  <div class="form-group">
    <label for="titleText">Title</label>
    <input type="text" class="form-control" name="title" value="" size=150 >
  </div>

  <div class="form-group">
    <label for="postText">Post</label>
    <textarea name="content" class="form-control" rows="8" cols="80"></textarea>
  </div>


  <button type="submit" name="Publish" value="clickPublish" class="btn btn-primary">Publish</button>
</form>

<%- include('partials/footer') -%>
